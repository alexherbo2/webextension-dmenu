{
  "manifest_version": 3,
  "name": "dmenu",
  "description": "Tab search, selection and beyond with a dynamic menu program",
  "version": "0.1.0",
  "permissions": [
    "tabs",
    "bookmarks",
    "history",
    "storage"
  ],
  "commands": {
    "tab-search": {
      "description": "Tab search",
      "suggested_key": {
        "default": "Ctrl+Q",
        "mac": "Command+Q"
      }
    },
    "bring-tab": {
      "description": "Bring tab",
      "suggested_key": {
        "default": "Ctrl+Shift+Q",
        "mac": "Command+Shift+Q"
      }
    },
    "open-bookmark": {
      "description": "Open bookmark",
      "suggested_key": {
        "default": "Alt+Q"
      }
    },
    "search-history": {
      "description": "Search history",
      "suggested_key": {
        "default": "Alt+Shift+Q"
      }
    }
  },
  "action": {
    "default_popup": "src/pages/options/index.html"
  },
  "options_ui": {
    "page": "src/pages/options/index.html",
    "open_in_tab": false
  },
  "icons": {
    "16": "packages/window-restore.png",
    "48": "packages/window-restore.png",
    "128": "packages/window-restore.png"
  }
}
